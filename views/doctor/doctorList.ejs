<main>
  <%- include('../partials/doctorProfileheaders');%>
    <center>

      <h1>Doctors List:</h1>
      <center>
        <div class="container doctorTable" id="doctorList">
          <%- include('../partials/messages');%>

            <table class="mt-4 p-5 rounded text-center responsive table " style="font-size: medium; font-weight: 600;">
              <thead style="font-size: large; font-weight: bold;">
                <tr>
                  <th>First Name</th>
                  <th>Last Name</th>
                  <th>Date Of Birth</th>
                  <th>Registration Number</th>
                  <th>Qualifications</th>
                  <th>Clinic Name</th>
                  <th>Languages</th>
                  <th>ABN:</th>
                </tr>
              </thead>
              <tbody id="table-body">

                <% doctors.forEach(function(doctor) { %>
                  <tr>
                    <td>
                      <%=doctor.firstName%>
                    </td>
                    <td>
                      <%=doctor.lastName%>
                    </td>
                    <td style="font-size: small;">
                      <%=doctor.dateOfBirth%>
                    </td>
                    <td>
                      <%=doctor.registrationNumber%>
                    </td>
                    <td>
                      <%=doctor.qualifications%>
                    </td>
                    <td>
                      <%=doctor.clinic%>
                    </td>
                    <td>
                      <%=doctor.languages%>
                    </td>
                    <td>
                      <%=doctor.abn%>
                    </td>
                    <td>
                    </td>
                  </tr>
                  <%});%>
              </tbody>
            </table>

        </div>
        <script src="../../models/doctorModel.js"></script>
        <script src="../../public/js/doctor/doctor.js"></script>
        <link rel="stylesheet" href="../../public/css/doctor/doctor.css" />
</main>