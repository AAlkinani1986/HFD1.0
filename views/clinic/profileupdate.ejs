                                <input disabled type="hidden" name="_id" value="<%=clinic.clinicId%>">

                              <div class="row">
                                <div class="input-field col s6">
                                  <input disabled placeholder="Clinicname" id="Clinicname" type="text" class="validate" name="ClinicName"
                                    value="<%=clinic.ClinicName%>">
                
