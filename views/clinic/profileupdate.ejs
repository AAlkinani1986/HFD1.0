    <div class="row">
        <div class="col s12 m6">
            <div class="card">
                <div class="card-image waves-effect waves-block waves-light">
                   
                    <div class="uploadImage">
    
                        <img class="activator" width="250"  name="clinic_img" src="../../public/data/uploads/clinic/<%=clinic.clinicId%>.jpg" id="clinic_img" accept="image/*"> 
   
                   </div>
                </div>
                <div class="card-content">
                    <span class="card-title activator grey-text text-darken-4">Clinic profile</span>
                    <p>
                        <label disabled for="textarea"><b>Clinic Services <%=clinic.textarea%></b></label> 
                    </p>
                </div>
                <div class="card-reveal">
                    <span class="card-title grey-text text-darken-4">Clinic profile
                        <i class="material-icons right">close</i></span>
